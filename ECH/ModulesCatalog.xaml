<modularity:ModuleCatalog
	xmlns = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x = "http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:modularity="clr-namespace:Microsoft.Practices.Prism.Modularity;assembly=Microsoft.Practices.Prism">

    <!-- 
	Ref: ProjectName.xap
	ModuleName: Name of .cs-file
	ModuleType: Ref(minus .xap).ModuleName, Ref(minus .xap)
	-->
    <modularity:ModuleInfo
		Ref = "ECH.ModuleA.xap"
		ModuleName = "ModuleA"
		ModuleType = "ECH.ModuleA.ModuleA, ECH.ModuleA" />

    <modularity:ModuleInfo
		Ref = "ECH.ModuleB.xap"
		ModuleName = "ModuleB"
		ModuleType = "ECH.ModuleB.ModuleB, ECH.ModuleB" />

    <modularity:ModuleInfo
		Ref = "ECH.ModuleC.xap"
		ModuleName = "ModuleC"
		ModuleType = "ECH.ModuleC.ModuleC, ECH.ModuleC" />

</modularity:ModuleCatalog>